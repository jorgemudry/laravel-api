{
    "name": "jorgemudry/laravel-api",
    "type": "project",
    "description": "A Laravel version to be used on new projects, with some tweaks to make it more API oriented.",
    "keywords": ["framework", "laravel", "api"],
    "license": "MIT",
    "require": {
        "php": "^8.1",
        "bugsnag/bugsnag-laravel": "^2.26",
        "guzzlehttp/guzzle": "^7.5",
        "laravel/framework": "^10.4.1",
        "laravel/sanctum": "^3.2.1",
        "laravel/tinker": "^2.8.1",
        "predis/predis": "^2.1.2"
    },
    "require-dev": {
        "barryvdh/laravel-ide-helper": "^2.13",
        "fakerphp/faker": "^1.21.0",
        "laravel/pint": "^1.6",
        "mockery/mockery": "^1.5.1",
        "nunomaduro/collision": "^7.2",
        "nunomaduro/larastan": "^2.5.1",
        "nunomaduro/phpinsights": "^2.8",
        "pestphp/pest": "^2.0.2",
        "pestphp/pest-plugin-laravel": "^2.0"
    },
    "autoload": {
        "psr-4": {
            "App\\": "app/",
            "Database\\Factories\\": "database/factories/",
            "Database\\Seeders\\": "database/seeders/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/"
        }
    },
    "scripts": {
        "post-autoload-dump": [
            "Illuminate\\Foundation\\ComposerScripts::postAutoloadDump",
            "@php artisan package:discover --ansi"
        ],
        "post-update-cmd": [
            "@php artisan vendor:publish --tag=laravel-assets --ansi --force"
        ],
        "post-root-package-install": [
            "@php -r \"file_exists('.env') || copy('.env.example', '.env');\""
        ],
        "post-create-project-cmd": [
            "@php artisan key:generate --ansi"
        ],
        "post-install-cmd": [
            "./dev/hooks/install"
        ]
    },
    "extra": {
        "laravel": {
            "dont-discover": []
        }
    },
    "config": {
        "optimize-autoloader": true,
        "preferred-install": "dist",
        "sort-packages": true,
        "allow-plugins": {
            "pestphp/pest-plugin": true,
            "php-http/discovery": true,
            "dealerdirect/phpcodesniffer-composer-installer": true
        }
    },
    "minimum-stability": "stable",
    "prefer-stable": true
}
