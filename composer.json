{
    "name": "jorgemudry/laravel-api",
    "type": "project",
    "description": "A Laravel version to be used on new projects, with some tweaks to make it more API oriented.",
    "keywords": ["framework", "laravel", "api"],
    "license": "MIT",
    "require": {
        "php": "^8.3",
        "bugsnag/bugsnag-laravel": "^2.29.0",
        "guzzlehttp/guzzle": "^7.10.0",
        "laravel/framework": "^12.31.1",
        "laravel/sanctum": "^4.2.0",
        "laravel/tinker": "^2.10.1",
        "predis/predis": "^2.4.0",
        "promphp/prometheus_client_php": "^2.14.1",
        "treblle/treblle-laravel": "^4.6.44"
    },
    "require-dev": {
        "fakerphp/faker": "^1.24.1",
        "larastan/larastan": "^3.7.2",
        "laravel/pail": "^1.2.3",
        "laravel/pint": "^1.25.1",
        "mockery/mockery": "^1.6.12",
        "nunomaduro/collision": "^8.8.2",
        "nunomaduro/phpinsights": "^2.13.1",
        "pestphp/pest": "^3.8.4",
        "pestphp/pest-plugin-laravel": "^3.2",
        "rector/rector": "^2.1.7"
    },
    "autoload": {
        "psr-4": {
            "App\\": "app/",
            "Database\\Factories\\": "database/factories/",
            "Database\\Seeders\\": "database/seeders/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/"
        }
    },
    "scripts": {
        "post-autoload-dump": [
            "Illuminate\\Foundation\\ComposerScripts::postAutoloadDump",
            "@php artisan package:discover --ansi"
        ],
        "post-update-cmd": [
            "@php artisan vendor:publish --tag=laravel-assets --ansi --force"
        ],
        "post-root-package-install": [
            "@php -r \"file_exists('.env') || copy('.env.example', '.env');\""
        ],
        "post-create-project-cmd": [
            "@php artisan key:generate --ansi",
            "@php -r \"file_exists('database/database.sqlite') || touch('database/database.sqlite');\"",
            "@php artisan migrate --graceful --ansi"
        ],
        "post-install-cmd": [
            "./dev/hooks/install"
        ],
        "dev": [
            "Composer\\Config::disableProcessTimeout",
            "npx concurrently -c \"#93c5fd,#c4b5fd,#fb7185,#fdba74\" \"php artisan serve\" \"php artisan queue:listen --tries=1\" \"php artisan pail --timeout=0\" \"npm run dev\" --names=server,queue,logs,vite"
        ],
        "test": [
            "@php artisan config:clear --ansi",
            "@php artisan test"
        ]
    },
    "extra": {
        "laravel": {
            "dont-discover": []
        }
    },
    "config": {
        "optimize-autoloader": true,
        "preferred-install": "dist",
        "sort-packages": true,
        "allow-plugins": {
            "pestphp/pest-plugin": true,
            "php-http/discovery": true,
            "dealerdirect/phpcodesniffer-composer-installer": true
        }
    },
    "minimum-stability": "stable",
    "prefer-stable": true
}
